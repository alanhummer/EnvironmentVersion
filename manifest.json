{
  "manifest_version": 2, 
  "name": "Environment Versions",
  "short_name": "EV",
  "description": "Get inventory of environment versions",
  "version": "1.0",
  "browser_action": {"default_icon": "images/icon.png","default_popup": "popup.html"},
  "permissions": [
    "https://*.atlassian.net/",
    "http://*.landsend.com/",
    "https://*.landsend.com/",
    "http://*.leinternal.com/",
    "storage",
    "downloads"
  ],
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
